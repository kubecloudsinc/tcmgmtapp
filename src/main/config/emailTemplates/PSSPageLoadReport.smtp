[subject]
{partner_name} : Page Load Times Report - {report_date}.
[body]
<html>
<body>
	<b><font face="Cambria" >Partner Support Service Page Load Times Report.</font></b>
	<br />
	<br />
	<br />
		<table border="1">
		<tbody>
			<tr>
				<th align="left" colspan="17" bgcolor="#E6E6FA"><font face="Cambria" >Partner Name</font></th>
				<td align="left"><font face="Cambria" >{partner_name}</font></td>
			<tr>		
			<tr>
				<th align="left" colspan="17" bgcolor="#E6E6FA"><font face="Cambria" >Customer Name</font></th>
				<td align="left"><font face="Cambria" >{customer_name}</font></td>
			<tr>
			<tr>
				<th align="left" colspan="17" bgcolor="#E6E6FA"><font face="Cambria" >Number of Inventories</font></th>
				<td align="left"><font face="Cambria" >{total_inventories}</font></td>
			<tr>
			<tr>
				<th align="left" colspan="17" bgcolor="#E6E6FA"><font face="Cambria" >Login User Name</font></th>
				<td align="left"><font face="Cambria" >{user_name}</font></td>
			<tr>			
			<tr>
				<th align="left" colspan="17" bgcolor="#E6E6FA"><font face="Cambria" >Login User Role</font></th>
				<td align="left"><font face="Cambria" >{user_roles}</font></td>
			<tr>						
		</tbody>
	</table>
	<br />
	<br />
	<font face="Cambria" >Listed below are time taken by various pages for the above Customer Inventory.</font>
	<br />
	<table border="1">
		<tbody>
			<tr>
				<th align="center" colspan="17" bgcolor="#E6E6FA"><font face="Cambria" >PAGE LOAD REPORT</font></th>
			<tr>
